<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.curation.mappers.UploadMapper">

	<update id="restore" parameterType="UserProfile">
		UPDATE ABOUT_ME SET USER_PROFILE_ROUTE = #{user_profile_route}
		WHERE USER_SEQ = #{user_seq};
	</update>
	
</mapper>
